class AI:
    def __init__(self):
        self.hp = 100
        self.attack_power = 10
        self.defense_power = 5

    def decide_action(self):
        if self.hp > 50:
            self.attack()
        else:
            self.defend()

    def attack(self):
        # 攻撃の実装
        print("Attacking...")

    def defend(self):
        # 防御の実装
        print("Defending...")


# 使用例
ai = AI()
ai.decide_action()
